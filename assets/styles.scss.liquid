  }
}

textarea {
  min-height: 100px;
}

/*================ Input element overrides ================*/
input[type="checkbox"],
input[type="radio"] {
  display: inline;
  margin: 0 8px 0 0;
  padding: 0;
  width: auto;
}


input[type="checkbox"] {
  @include appearance(checkbox);
}
input[type="radio"] {
   @include appearance(radio);
}

input[type="checkbox"], input[type="radio"]{
    display:none;
}
input[type="radio"] + label, input[type="checkbox"] + label {
  font-size:14px;
  text-transform:capitalize;
  color:$colorTextSecondary;
  line-height: 18px;
}


input[type="checkbox"] + label span, input[type="radio"] + label span {
    display:inline-block;
    width: 15px;
    height: 15px;
    margin: -4px 10px 0 0;
    vertical-align:middle;
    border: 1px solid $colorTextSecondary;
    cursor:pointer;
    position:relative;
    &:after{
      position:absolute;
        content:'';
        width:auto;
        height:auto;
        top:1px;
        left:1px;
        right:1px;
        bottom:1px;
        background-color:transparent;
    }
}

input[type="checkbox"]:checked + label span {
  border:1px solid $colorPrimary;
    &:after{
    background-color:$colorPrimary;
    }
}

input[type="radio"] + label span {
  border-radius:50%;
}

input[type="radio"]:checked + label span {
  border:1px solid $colorPrimary;
  &:after{
    background-color:$colorPrimary;
    top:2px;
    left:2px;
    right:2px;
    bottom:2px;
    border-radius:50%;
  }
}


input[type="image"] {
  padding-left: 0;
  padding-right: 0;
}

select {
  background-position: right center;
  background: {
    image: url(dwn-arrow_black.png);
    repeat: no-repeat;
    position: right 10px center;
    color: darken($colorBody , 2);
    size:8px;
  }
  color:$colorTextSecondary;
  padding-right: 28px;
  text-indent: 0.01px;
  text-overflow: '';
  cursor: pointer;

  /*================ Hide the svg arrow in IE9 and below ================*/
  .ie9 &,
  .lt-ie9 & {
    padding-right: 10px;
    background-image: none;
  }
}
optgroup {
  font-weight: bold;
}

// Force option color (affects IE only)
option {
  color: #000;
  background-color: #fff;
}

select::-ms-expand {
  display: none;
}

/*================ Form labels ================*/
.hidden-label {
  position: absolute;
  height: 0;
  width: 0;
  margin-bottom: 0;
  overflow: hidden;
  clip: rect(1px, 1px, 1px, 1px);

  // No placeholders, so force show labels
  .ie9 &,
  .lt-ie9 & {
    position: static;
    height: auto;
    width: auto;
    margin-bottom: 2px;
    overflow: visible;
    clip: initial;
  }
}


/*================ Error styles ================*/
input,
select,
textarea {
  &.error {
    border-color: $errorRed;
    background-color: $errorRedBg;
    color: $errorRed;
  }
}

label.error {
  color: $errorRed;
}


/*================ Input Group ================*/
.input-group {
  position: relative;
  display: table;
  border-collapse: separate;

  input {    
    &::-moz-focus-inner {
      border: 0;
      padding: 0;
      margin-top: -1px;
      margin-bottom: -1px;
    }
  }
}

.input-group-field,
.input-group-btn {
  display: table-cell;
  vertical-align: middle;
  margin: 0;
}

.input-group .btn,
.input-group .input-group-field {
  height: 37px;
}

.input-group .input-group-field {
  width: 100%;
}

.input-group-btn {
  position: relative;
  white-space: nowrap;
  width: 1%;
  padding: 0;
}


/*============================================================================
  #Pagination
==============================================================================*/

.pagination-wrapper{ 
  .pagination {
    text-align: center;

    > li {
      display: inline-block;
      margin: 0 2px;
      margin-top:40px;
      
      @include at-query ($medium , $large){
        margin-top:30px;
      }
      @include at-query ($max , $preMedium){
        margin-top:25px;
      }
      a{    
        display:block;
        border:2px solid $colorBorder;
        color:$colorTextSecondary;
        padding: 4px 12px;
        line-height: 1;
        border-radius: 20px;
        width: 46px;
        font-weight: 500;
        font-size: 14px;
        font-weight: 500;
        @include transition(0.4s ease-in-out);
        &.fa{
          font-family: 'FontAwesome'; 
        }
        
      }
      
      &.page.current{
        color:$colorPrimary; 
        border-color:$colorBorder;
        background-color:$colorBorder;
        padding: 4px 12px;
        line-height: 1;
        border:2px solid $colorBorder;
        border-radius: 20px;
        width: 46px;
      }
      
      &:hover{
        border-color:$colorPrimary;
        a{
          color:$colorTextBody;
        }
      }
    }  
  }
}


/*============================================================================
  #Breadcrumbs
==============================================================================*/
.breadcrumb {
  a,
  span {
    display: inline-block;
    padding: 0 6px 0 0;
    margin-right: 6px;
    color:$colorSecondary;
    &:after{
      content:'\f105';
      font-family: 'FontAwesome';
      padding-left:12px;
      font-size:14px;
    }
    &:first-child {
      padding-left: 0;
    }
    &:last-child {
      color:$colorBody;
      padding:0;
      margin-right:0;
      &:after{
        display:none;   
      }
    }
  }
}


/*============================================================================
  #Notes and Form Feedback
==============================================================================*/
.note,
.errors {
  @include border_radius(3px);
  padding: 6px 12px;
  margin-bottom: $gutter / 2;
  border: 1px solid transparent;
  font-size: 0.9em;
  text-align: left;
  margin:15px 0;
  ul,
  ol {
    margin-top: 0;
    margin-bottom: 0;
  }

  li:last-child {
    margin-bottom: 0;
  }

  p {
    margin-bottom: 0;
  }
}

.note {
  border-color: $colorBorderPrimary;
}

.errors {
  ul {
    list-style: disc outside;
    margin-left: 20px;
  }
}

.form-success {
  color: $successGreen;
  background-color: $successGreenBg;
  border-color: $successGreen;

  a {
    color: $successGreen;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}

.form-error,
.errors {
  color: $errorRed;
  background-color: $errorRedBg;
  border-color: $errorRed;

  a {
    color: $errorRed;
    text-decoration: underline;

    &:hover {
