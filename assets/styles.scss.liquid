  margin-bottom: 0;
  padding: 69px 0 39px;
  @include at-query ($postLarge , $maxlarge){
    padding: 49px 0 29px;
  }
  @include at-query ($medium , $large){
    padding:45px 0 30px;
  }
  @include at-query ($max , $preMedium){
    padding:25px 0 25px;
  }

}


/*--======  #PRODUCT CATEGORIES  ======*/

#categories{
  .grid{
    margin:0;
    .grid__item{
      padding:0;
      .categories_item {
        position: relative;
        overflow: hidden;
        text-align: center;
        cursor: pointer;
        display:block;
        .categories_img {
          width: 100%;
          height: 360px;
          @include at-query ($medium , $maxlarge){
            height:260px;
          }
          @include at-query ($postSmall , $preMedium){
            height:160px;
          }
          @include at-query ($max , $small){
            height:120px;
          }
          img {
            position: relative;
            display: block;
            height: 100%;
            width: 100%;
            object-fit: cover;
          }
        }
        .categories_info{
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          text-transform:uppercase;
          &:before {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: -webkit-linear-gradient(top, rgba(37,37,37,0) 0%, rgba(37,37,37,0.8) 75%);
            background: linear-gradient(to bottom, rgba(37,37,37,0) 0%, rgba(37,37,37,0.8) 75%);
            content: '';
            opacity: 0;
            @include transform(translate3d(0,50%,0));
            @include transition(all 0.4s ease-in-out);
          }
          p, .btn--secondary{
            position: absolute;
            bottom: 0;
            left:50%;
            margin:0;
            opacity:0;
            @include at-query ($max , $large){
              display:none;
            }
          }
          p{
            padding:0;
            color:$colorBody;
            width: 80%;
            @include transition(all 0.5s ease-in-out);
            @include transform(translate3d(-50%,-80px,0));
            @include at-query ($postLarge , $maxlarge){
              width:90%;
              font-size:12px;
            }
            @include at-query ($medium , $large){
              width:80%;
            }
            @include at-query ($max , $preMedium){
              display:none;
            }
          }
          .btn--small{
            @include transform(translate3d(-50%,-10px,0));
            @include transition(all 0.6s ease-in-out);
          }
          h3{
            position: absolute;
            top: 50%;
            left: 0;
            margin:0;
            width: 100%;
            color: #cab901;
            @include transition(all 0.4s ease-in-out);
            @include transform(translate3d(0,-50%,0));
            @include at-query ($max , $small){
              font-size:18px;
              font-weight:400;
            }
          }
        }
        &:hover{
          .categories_info{
            &:before{
              opacity: 1;
              @include transform(translate3d(0,0px,0));
            }
            h3{
              color: $colorBody;
              @include transform(translate3d(0,-50%,0) translate3d(0,-40px,0));
              @include at-query ($postLarge , $maxlarge){
                padding-bottom:20px;
                @include transform(translate3d(0,-50%,0) translate3d(0,-50px,0));
              }
              @include at-query ($max , $large) {
                @include transform(translate3d(0, -50%, 0));
              }
            }
            p {
              opacity: 1;
              @include transform(translate3d(-50%,-120px,0));
              @include at-query ($postLarge , $maxlarge){
                @include transform(translate3d(-50%,-105px,0));
              }
              @include at-query ($max , $large) {
                @include transform(none);
              }
            }
            .btn--secondary{
              opacity: 1;
              @include transform(translate3d(-50%,-60px,0));
              @include at-query ($postLarge , $maxlarge){
                @include transform(translate3d(-50%,-50px,0));
              }
              @include at-query ($max , $large) {
                @include transform(none);
              }
            }
          }
        }
      }
    }
  }
}


/*========================  #PRODUCT LIST  ========================*/



#collection{
  position:relative;
  .grid{
    margin:0;
    .grid__item{
      padding:0;
    }
  }
  @include at-query ($medium , $maxMedium){
    > .container{
      > .products_grid{
        > .grid{
          > .grid__item{
            width:33.33%;
          }
        }
      }
    }
  }
  &.best_sellers, &.related_product{
    .product_list{      
      .grid__item{
        @include at-query($medium , $maxMedium){
          width:33.33%;
          &:last-child{          
            display:none;
          }
        }
      }
    }
  }
}

#collection{
  @include at-query ($medium , $maxMedium){
    > .container{
      > .product_list{
        &.grid{
          > .grid__item{
            width:33.33%;
          }
        }
      }
    }
  }
}


.product_item {
  position: relative;
  text-align: left;
  border: 1px solid $colorBorder;
  background-color:$colorBody;
  margin:0 -1px -1px 0;
  @include at-query ($max , $small){
    margin-bottom:20px;
  }
  .price {
    font-weight: 700;
    font-size:16px;
    color:$colorTextBody;
    position: relative;
    float:left;
    z-index: 999;
    margin: 12px 0 0 25px;
    @include at-query ($max , $maxlarge){
        margin-left:12px;    
    }
    @include at-query ($max , $large){
      font-size:14px;
      line-height:20px;
      font-weight:500;
    }
    span{
      //float:left;
      margin-left:2px;
      vertical-align: bottom;
      &.money{
      line-height:20px;
      }
    }

    .compare{
      margin-left: 8px;
      span{
        font-size:12px;
        color:$colorTextSecondary;
        text-decoration: line-through;
        @include at-query ($max , $preMedium){
          font-size:10px;
        }  
      }
    }
  }
  .sale_tag{
    font-size:12px;
    font-weight:700;
    text-transform:uppercase;
    border-top:1px solid $colorBorder;
    border-bottom:1px solid $colorBorder;  
    display:inline-block;
    line-height:22px;
    color:$colorTextBody;
    padding: 2px 5px 0 20px;
    position: absolute;
    top: 12px;
    right: 0;
    @include at-query ($max , $maxlarge){
      font-size:10px;
      font-weight:500;
      line-height:20px;
    }
    @include at-query ($max , $large){
      top:8px;
    }
    &:after,
    &:before{
      content: '';
      width: 18px;
      height: 1px;
      display: inline-block;
      background: $colorBorder;
      position: absolute;
      left: -2px;
      @include at-query ($max , $maxlarge){
        width:16px;
      }
    }
    &:before {
      top: 6px;    
      @include transform(rotate(45deg));  
      @include at-query ($max , $maxlarge){
        top: 5px;
      } 
    }
    &:after{
      bottom: 5px;
      @include transform(rotate(-45deg)); 
      @include at-query ($max , $maxlarge){
        bottom: 4px;
      }   
    } 
  }


  .product_img{
    position: relative;
    display: block;
    z-index: 99;
    background: #fff;
    width: calc(100% - 2px);
    margin: 12px auto 0;
    @include flexbox();
    @include align-items(center);
    > a{
      display:block;
      width:100%;
    }
    img{
      display: block;
      max-width: 70%;
      margin: auto;
    }
    .placeholder-svg{
        border:none;
      }
  }
  .product_info {
    padding: 16px 25px;
    position:relative;
    z-index:99;
    //overflow:hidden;
    border-top: 1px solid $colorBorder;
    @include flexbox();
    @include align-items(center);
    @include clearfix;
    @include at-query ($postLarge , $maxlarge){
      padding: 12px 16px;
    }
    @include at-query ($postSmall , $large){
      padding: 8px 10px
    }  
    @include at-query ($max , $small){
      padding:10px 15px;
    }
    &:before{
      position: absolute;
      top:-1px;
      left: 0px;
      right:0px;
      content: '';
      opacity: 1;
      @include transition(opacity 0.35s ease-in-out, transform 0.35s ease-in-out);
      border-top: 1px solid $colorSecondary;
      @include transform(scale(0,1));
    }
    > div{      
      .view_btn{
        position: absolute;
        width: calc(100% - 2px);
        height:calc(100% - 2px);
        top:1px;
        left:1px;
        background: #fff;
        text-transform: uppercase;
        opacity: 0;
        visibility:hidden;
        top:60%;
        @include transform(translateY(-50%)); 
        @include transition(all 0.35s ease-in-out);
        @include at-query ($max , $large){
          display:none;
        }
        a{
          font-size: 16px;
          line-height: 30px;            
          font-weight: 500;
          color:$colorSecondary;
          display:inline-block;
          border-bottom:2px solid $colorSecondary;
          position:absolute;         
          left:50%;
          top:50%;
          @include transform(translate(-50% , -50%));
          @include at-query ($max , $preMedium){
            font-size: 15px;
            line-height: 2em;
          }
        }
      }
      > h1{
        line-height:24px;
        padding-right:15px;
        margin-bottom:0;
        text-align:left;
        text-transform: capitalize;
        display:inline;
        font-size: inherit;
        opacity:1;
        display:block;
        @include transition(transform 0.35s ease-in-out);
        @include at-query ($medium , $maxlarge){
          line-height: 20px;
        }
        @include at-query ($max , $preMedium){
          line-height: 18px;
        }
        a{
          font-size:14px;
          font-weight:400;
          @include at-query ($medium , $maxlarge){
            font-size: 12px;
          }
          @include at-query ($max , $preMedium){
            font-weight: 500;
            display:block;
            font-size: 12px;
          }
        }
      }
    }
    .fa{
      color:$colorSecondary;
      padding:0;
      @include at-query ($medium , $maxlarge){
        font-size:18px;
      }
      @include at-query ($max , $preMedium){
        font-size:16px;
      }
    }
    &:hover{
      > div{
        > #product-title{
          opacity:0;
          @include at-query ($max , $large){
            opacity:1;
          }           
        }         
      }
      .view_btn{
        opacity:1;
        visibility:visible;   
        top:50%;
      }
      &:before{
        opacity: 1;
        @include transform(scale(1));
      }
    }
  }
  .border_effect {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index:9;
    &:before, &:after{
      position: absolute;
      top: -1px;
      right: -1px;
      bottom:-1px;
      left: -1px;
      content: '';
      opacity: 1;
      @include transition(opacity 0.35s, transform 0.5s);
    }
    &:before {
      border-top: 1px solid $colorSecondary;
      border-bottom: 1px solid $colorSecondary;
      @include transform(scale(0,1));
    }
    &:after {
      border-right: 1px solid $colorSecondary;
      border-left: 1px solid $colorSecondary;
      @include transform(scale(1,0));
    }
  }
  &:hover{
    .border_effect{
      &:before, &:after{
        opacity: 1;
        @include transform(scale(1));
      }
    }
  }
}


