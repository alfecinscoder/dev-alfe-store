/****=================  COLOR SWATCH   ===============****/
   
.swatch{
  @include clearfix;
  margin-bottom:25px;
  @include at-query ($max , $preMedium){
  margin-bottom:18px;
  }
  .header{
    font-weight:900;
    text-transform: uppercase;
    float: left;
    width: 75px;
    line-height: 32px;
    font-size: 12px;
    @include at-query ($max , $preMedium){
      line-height:26px;
    }
  }
  @include at-query ($medium , $maxlarge){
    margin-bottom:18px;     
  }
  .swatch-element{
    float:left;
    position:relative;
    input[type="radio"]{
      display:none;
      &:checked { 
        + label{ 
          border:2px solid $colorPrimary;          
        }
      }
    }
    label{
      float: left;
      position:relative;
      margin-right: 5px;
      width: 50px;
      height: 32px;
      border: 2px solid $colorBorderPrimary;
      border-radius: 16px;
      @include at-query ($max , $preMedium){
      width:44px;
        height:26px
      }
      span{
        width: 40px;
        height: 24px;
        border:0 !important;
        display: inline-block;
        text-align:center;
        position:relative;        
        @include border_radius(12px);
        top: 6px;
        left: 3px;
        position:absolute;
        cursor:pointer;  
        @include at-query ($max , $preMedium){
          width:34px;
          height:18px
        }
        &:after{
          display:none;
        }
      }
    }
  }     
}



.swatch {
  .tooltip {
    text-align:center;
    background:$colorBorderPrimary;
    color:$colorPrimary;
    bottom:100%;
    padding:6px 10px 2px;
    display:block;
    position:absolute;
    text-transform:uppercase;
    width:100px;
    font-size: 12px;
    line-height:20px;
    font-weight: 900;
    left:50%;
    margin-bottom:15px;
    opacity:0;
    visibility:hidden;
    @include transform(translate(-50% , 10px));
    @include transition(all .25s ease-out);
    @include box-shadow(2px 2px 6px rgba(0, 0, 0, 0.28));
    z-index: 10000;
    &:before {
      bottom:-20px;
      content:" ";
      display:block;
      height:20px;
      left:0;
      position:absolute;
      width:100%;
    }
    &:after {
      border-left:solid transparent 10px;
      border-right:solid transparent 10px;
      border-top:solid $colorBorderPrimary 10px;
      bottom:-10px;
      content:" ";
      height:0;
      left:50%;
      margin-left:-13px;
      position:absolute;
      width:0;
    }
  }
  .swatch-element{
    &:hover{
      .tooltip {
        opacity:1;
        visibility:visible;
        @include transform(translate(-50% , 0px));
      }
    }
  }
  &.error{
    background-color:#E8D2D2!important;
    color:#333!important;
    padding:1em;
    border-radius:5px;
    p {
      margin:0.7em 0;
      &:first-child {
        margin-top:0;
      }
      &:last-child {
        margin-bottom:0;
      }

    }
  }
}




