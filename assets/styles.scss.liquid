/*============================================================================
  #NEWSLETTER POPUP
==============================================================================*/


.overlay.pop_class{
  @include transform(scale(1));
  opacity:1;
  @include transition(all 0.7s);
}
.newsletter_overlay{
  position: fixed;
  height: 100%;
  width: 100%;
  opacity: 0;
  z-index: -1;
  top: 0;
  background: rgba(38, 36, 35, 0.9);
  @include transition(all 0.5s);
  
  &.newsletter_popupactive{
    opacity: 1;
    z-index: 9999999;
  }
  .pop_up_overlay{
    position: absolute;
    height: 100%;
    width: 100%;
    z-index: 9;
    top: 0;
    left:0;
    background: rgba(0,0,0,0.7);
    @include at-query ($max , $preMedium){
      display:block;
    }
  }
  
  .newsletter_popup{
    padding:10px;
    background-color:#333;
    background-size:cover;
    background-repeat:no-repeat;
    width: 460px;
    height: 400px;
    position: absolute;
    top: 50%;
    left: 50%;
    @include transform(translate(-50%,-50%));   
    @include at-query ($postSmall , $preMedium){
      width: 80%;
      height: 300px;
    }
    @include at-query ($max , $small){
      width: 90%;
      height: 340px;
    }
    .close_btn{
      position:absolute;
      top: 10px;
      right: 10px;
      cursor:pointer;
      z-index: 99999999;
      &:after{
        content:'';
        width:20px;
        height:20px;
        background:url(cross_white.png) center no-repeat; 
        display:block;
        background-size:100%;       
      }
    }
    form{
      color:$colorBody;
      text-align: center;
      position:relative;
      top: 50%;
      width:100%;
      padding: 0 60px;
      float:right;
      z-index: 9999999;
      @include transform(translateY(-50%));   
      @include at-query ($postSmall , $preMedium){
      padding:0 30px;
      }
      @include at-query ($max , $small){
        padding:0;
      }
      h6{
        text-transform: capitalize;
        font-weight: 400;
        font-size: 15px;
      }
      h2{       
        margin-bottom: 7px;
        line-height: 1;
        font-weight: 700;
        font-size: 60px;
        letter-spacing: 6px;
        @include at-query ($max , $preMedium){
          font-size:40px;
        }
      }
      .social_box{
        margin-top:30px;
        @include at-query ($max , $preMedium){
          margin-top:20px;
        }
        ul{
          list-style:none;
          li{
            display:inline-block;
            margin:0 5px;
            a{
              @include circle(30px , $colorBody);
              display:inline-block;
              .fa{
                font-size:18px;
                line-height: 32px;
              } 
              &:hover{
                background-color:$colorBodySecondary;                
              }
            }
          } 
        }
      }   
      .filed_grp{
        margin-top: 45px;
        @include clearfix();
        @include at-query ($max , $preMedium){
          margin-top:20px;
        }
        input[type="email"]{  
          border-radius: 4px;
          -moz-border-radius: 4px;
          -ms-border-radius: 4px;
          -webkit-border-radius: 4px;
          height: 38px;
          padding: 5px 15px 7px;
          line-height: 26px;
          @include placeholder ($FontSecondary , 13px , $colorTextBody , capitalize , normal , 1 , 400);
          @include placeholder-focus;
        }
        input[type="submit"]{  
          border-radius: 4px;
          -moz-border-radius: 4px;
          -ms-border-radius: 4px;
          -webkit-border-radius: 4px;          
          background-color: $colorSecondary;
          color: $colorBody;
          border-color: $colorSecondary;
          padding: 8px 40px 6px;
          &:hover{                 
            background-color: $colorPrimary;
            border-color: $colorPrimary;
          }
        }
      }
    }
  }
}



