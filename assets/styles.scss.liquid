/*============================================================================
  #Breakpoint and Grid Variables
==============================================================================*/
$viewportIncrement: 1px;

$small: 480px;
$medium: 768px;
$maxMedium: 991px;
$large: 1024px;
$maxlarge: 1366px;

$postSmall: $small + $viewportIncrement;
$preMedium: $medium - $viewportIncrement;
$postMedium: $medium + $viewportIncrement;
$postLarge: $large + $viewportIncrement;
$postmaxlarge: $maxlarge + $viewportIncrement;

/*================ The following are dependencies of csswizardry grid ================*/
$breakpoints: (
  'small' '(max-width: #{$small})',
  'medium' '(min-width: #{$postSmall}) and (max-width: #{$preMedium})',
  'medium-down' '(max-width: #{$preMedium})',
  'medium-large' '(min-width: #{$medium}) and (max-width: #{$large})',
  'max-large' '(min-width: #{$postLarge}) and (max-width: #{$maxlarge})',
  'large' '(min-width: #{$postmaxlarge})',
  'large-down' '(min-width: #{$postLarge})'
);
$breakpoint-has-widths: ('small', 'medium', 'medium-down' , 'medium-large' , 'max-large' , 'large' , 'large-down');
$breakpoint-has-push:  ('medium', 'medium-down' , 'medium-large' , 'max-large' , 'large' , 'large-down');
$breakpoint-has-pull:  ('medium', 'medium-down' , 'medium-large' , 'max-large' , 'large' , 'large-down');

