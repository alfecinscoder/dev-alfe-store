/*==============  slider controls  ================*/


.bx-wrapper {
  .bx-controls-direction {
    a {
      position: absolute;
      z-index: 9999;
      margin:0px 10px 0;
      top:50%;        
      width: 50px;
      height: 50px;
      background:none;
      padding: 6px;
      @include transform(translateY(-50%));
      @include at-query ($max , $preMedium){
        padding: 3px;
        width:30px;
        height:30px;
        margin:0;
      }
      &.disabled{
        display: none;
      }
      &:before {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        border-radius: 50%;
        background: #252525;
        content: '';
        opacity: 0;
        @include transition(transform 0.3s , opacity 0.3s);
        @include transform(scale(0.9));
      }
      span {
        position: relative;
        display: block;
        margin:0;
        width: 100%;
        height:100%;
        &:before,
        &:after {
          position: absolute;
          left: 25%;
          width: 3px;
          height: 50%;
          background: #252525;
          content: '';
          @include transition(transform 0.3s , background-color 0.3s);
          @include backface(hidden);
        }
        &:before {
          @include transform(translateX(-50%) rotate(30deg));
          @include transform-origin(0 100%);
        }
        &:after {
          top: 50%;
          @include transform(translateX(-50%) rotate(-30deg));
          @include transform-origin(0 0);
        }
      }
      &.bx-prev{ 
        left:10px;
        @include at-query ($max , $preMedium){
          left:2px;
        }
      }
      &.bx-next{ 
        right:10px;
        @include at-query ($max , $preMedium){
          right:2px;
        }
        span {
          @include transform(rotate(180deg));
        }
      }
      &:hover{
        &:before {
          opacity: 1;
          @include transform(scale(1));
        }
        span{
          &:before,
          &:after {
            background: #fff;
          }
          &:before {
            @include transform(translateX(-50%) rotate(45deg));
          }

          &:after {
            @include transform(translateX(-50%) rotate(-45deg));
          }
        }
      }
    }
  }
}


