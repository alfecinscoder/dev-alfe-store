/*============================================================================
  #404 ERROR 
==============================================================================*/

.page_content_404{
  display: flex;
  align-items: center;
  height: 100vh;
  background-size:cover !important;
  background-position:50% 50% !important;
  
 &:after{
   content:'';
   background-color:rgba(0, 0, 0, 0.52);
   position:absolute;
   top:0;
   left:0;
   width:100%;
   height:100%;        
   z-index:9;
 }
  @include at-query($max , $preMedium){
    height: auto;
    padding: 40px 0;
  }
  .error-content{
    text-align: center;
    width: 100%;
    position:relative;
    z-index:99;
    color:$colorBody;
    .brand-name{
      width: 300px;
      margin: 0 auto 60px;
      @include at-query($medium , $maxlarge){
          width:260px;
          margin: 0 auto 30px;
      }
      @include at-query($max , $preMedium){
          width:180px;
          margin: 0 auto 30px;
      }
      a{
      display:block;
      }
    }
    .error_msg{
      width: 100%;
      max-width: 768px;
      margin: 0 auto;
      @include at-query($max , $preMedium){
      padding:0 20px;
      }
      h1{
        font-size: 260px;
        margin-bottom: 60px;
        letter-spacing: 0.1em;
          @include at-query($medium , $maxlarge){
            font-size: 180px;
            margin-bottom: 50px;
          }
          @include at-query($max , $preMedium){
            font-size: 80px;
            margin-bottom: 30px;
          }
      }
      h6{
      letter-spacing:6px;
        line-height:1.3;
        text-transform:uppercase;
        margin-bottom:20px;
        @include at-query($max , $preMedium){
          letter-spacing:3px;
        }
      }
      p{
      margin-bottom:30px;
        @include at-query($max , $preMedium){
          margin-bottom:25px;
          line-height:22px;
        }
      }
      .btn--primary{
      background-color:$colorBody;
        border-color:$colorBody;
        color:$colorPrimary;
        &:hover{
          background-color:$colorSecondary;
          border-color:$colorSecondary;
          color:$colorPrimary;
        }
      }
    }    
    .bottom_social{
      margin-top:40px;
      .fa{
      color:$colorSecondary;
        &:hover{
          color:$colorBody;
        }
      }
      a{
      font-size:20px;
        margin:0 7px;
      }
    }
  }
}


